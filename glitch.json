{
  "projectType": "generated_static",
  "buildDirectory": "build",
  "install": "npx -y npm@latest ci",
  "start": "npx -y npm@latest npm run ide:csb",
  "watch": {
    "install": {
      "install": {
        "include": [
          "^package\\.json$"
        ],
        "exclude": []
      },
      "restart": {
        "include": [
          "^\\.env$",
          "^backend/"
        ],
        "exclude": [
          "src/",
          "stories/"
        ]
      },
      "noSavedEvents": true
    }
  }
}
